{
  "mcpServers": {
    "sequential-thinking": {
      "command": "npx",
      "args": [
        "-y",
        "@modelcontextprotocol/server-sequential-thinking"
      ]
    },
    "firewalla": {
      "command": "node",
      "args": ["./dist/server.js"],
      "env": {
        "FIREWALLA_MSP_TOKEN": "${FIREWALLA_MSP_TOKEN}",
        "FIREWALLA_MSP_ID": "${FIREWALLA_MSP_ID}",
        "FIREWALLA_BOX_ID": "${FIREWALLA_BOX_ID}"
      }
    }
  }
}